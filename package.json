{
  "name": "flat-notifier",
  "version": "0.1.0",
  "bin": {
    "flat-notifier": "bin/flat-notifier.js"
  },
  "scripts": {
    "dev": "ts-node --files src/ebay/local.ts",
    "dev:offline": "STAGE=offline ts-node --files src/ebay/local.ts",
    "dev:discord": "ts-node --files src/ebay/discord.ts",
    "build": "rm -rf dist && tsc && cp src/ebay/*ock.html dist/src/ebay/",
    "compile": "loadnvm && tsc",
    "watch": "tsc -w",
    "cdk": "cdk",
    "test": "jest",
    "predeploy": "rm -rf dist && tsc",
    "deploy": "cdk deploy"
  },
  "devDependencies": {
    "@aws-cdk/aws-dynamodb": "^1.134.0",
    "@types/jest": "^27.0.3",
    "@types/jsdom": "^16.2.13",
    "@types/ws": "^8.2.2",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "aws-cdk": "^1.134.0",
    "aws-sdk": "^2.1058.0",
    "discord.js": "^12.5.3",
    "eslint": "^8.4.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-plugin-import": "^2.25.3",
    "jest": "^27.4.4",
    "jsdom": "^19.0.0",
    "source-map-support": "^0.5.21",
    "ts-jest": "^27.1.1",
    "ts-node": "^10.4.0",
    "typescript": "~4.5.3"
  },
  "dependencies": {
    "@aws-cdk/aws-events": "^1.134.0",
    "@aws-cdk/aws-events-targets": "^1.134.0",
    "@aws-cdk/aws-lambda": "^1.134.0",
    "@aws-cdk/core": "^1.134.0",
    "@types/aws-lambda": "^8.10.89"
  }
}